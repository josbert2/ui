{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@blocks/*": ["src/blocks/*"],
      "@styles/*": ["src/styles/*"],
      "@lib/*": ["src/lib/*"],
      "@tailus-ui/*": ["src/components/tailus-ui/*"],
      "@layouts/*": ["src/layouts/*"],
      "@content/*": ["src/content/*"],
      "@store/*": ["src/store/*"],
      "@types/*": ["src/types/*"],
      "@hooks/*": ["src/hooks/*"],
    }
  },
}